RewriteRule ^clip/(.+) index2.php?f=video&id=$1

RewriteRule ^inscripcio2011 index.php?f=inscripcio2011
RewriteRule ^carta_president index.php?f=cartapresident



RewriteRule ^usuari/login index.php?f=userLogin
RewriteRule ^usuari/registre index.php?f=userRegister
RewriteRule ^usuari/registrat index.php?f=userRegistered
RewriteRule ^usuari/edita/(.+) index.php?f=userEdit&result=$1
RewriteRule ^usuari/edita index.php?f=userEdit








