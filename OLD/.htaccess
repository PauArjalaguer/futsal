RewriteRule ^competicio index.php?f=competicions
RewriteRule ^divisio/(.+)/(.+) index.php?f=competicio&title=$1&round=$2
RewriteRule ^divisio/(.+) index.php?f=competicio&title=$1
RewriteRule ^playoff/(.+) index.php?f=playoff&title=$1
RewriteRule ^copa/(.+)/(.+) index.php?f=copa&title=$1&round=$2
