RewriteRule ^calendari index.php?f=agenda

RewriteRule ^documents/(.+)/(.+) index.php?f=documents&title=$1&p=$2
RewriteRule ^documents/(.+) index.php?f=documents&title=$1

RewriteRule ^imatges/etiqueta/(.+)/(.+) index2.php?f=fotos&tag=$1&page=$2
RewriteRule ^imatges/etiqueta/(.+) index2.php?f=fotos&tag=$1

RewriteRule ^imatges/photoset/(.+)/(.+) index2.php?f=fotos&photoset=$1&page=$2
RewriteRule ^imatges/photoset/(.+) index2.php?f=fotos&photoset=$1

