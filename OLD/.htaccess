RewriteRule ^categoria/(.+) index.php?f=news_inicial&tag=$1
RewriteRule ^noticia/(.+) index.php?f=news_detail&title=$1
RewriteRule ^junta index.php?f=junta
RewriteRule ^estatuts index.php?f=estatuts
